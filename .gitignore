# Dev only
web/.htaccess

# Cache and logs (Symfony2)
/app/cache/*
/app/logs/*
!app/cache/.gitkeep
!app/logs/.gitkeep

# Parameters
/app/config/parameters.yml

# Managed by Composer
/app/bootstrap.php.cache
/var/bootstrap.php.cache
/bin/*
!bin/console
!bin/symfony_requirements
/vendor/

# Assets and user uploads
/web/bundles/
/web/uploads/*
!/web/uploads/.gitkeep
/web/uploads/media/*
!/web/uploads/media/.htaccess
/web/formsubmissions/*
!/web/formsubmissions/.gitkeep
web/css
web/Kunstmaan*

# Composer PHAR
/composer.phar

# Managed by bower/node
/web/vendor
/app/Resources/node_modules/*
!/app/Resources/node_modules/.gitkeep
/app/Resources/.sass-cache

# Managed by capifony
/backups

# Grunt build files
src/Ten24/ReginaFolkFestival/WebsiteBundle/Resources/build/.temp/*

# IDE
# Covers JetBrains IDEs: IntelliJ, RubyMine, PhpStorm, AppCode, PyCharm

*.iml

